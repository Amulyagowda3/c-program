  scanf("%f %f %f %f %f",&s[i].m1,&s[i].m2,&s[i].m3,&s[i].m4,&s[i].m5);
  }
   for(i=0;i<n;i++)
   {
   s[1]avg=(s[i].m1+s[i].m2+s[i].m3+s[i].m4+s[i].m5)/5.0,
   }
printf("student scored above avg");
printf("\n USN\t studentName\t Average");
for (1=0;i<;i++)
{
 if(s[i]:avg>50.0)
  {
printf("\nUSN\tstudentName\tAverage");
  }
printf("students scored below avg");
printf("In USN\tAverage");
for(i=0;i<0:i++)
